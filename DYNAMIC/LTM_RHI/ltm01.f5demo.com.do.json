{
    "schemaVersion": "1.23.0",
    "class": "Device",
    "async": true,
    "label": "Basic onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "ltm01.f5demo.com",
        "myLicense": {
            "class": "License",
            "licenseType": "regKey",
            "regKey": "<License-Key>",
            "overwrite": true
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
              "4.2.2.2",
              "8.8.8.8"
            ],
            "search": [
                "f5demo.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
              "0.pool.ntp.org",
              "1.pool.ntp.org",
              "2.pool.ntp.org"
            ],
            "timezone": "America/Los_Angeles"
        },
        "admin": {
            "class": "User",
            "userType": "regular",
            "shell": "bash"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal"
        },
        "internal": {
            "class": "VLAN",
            "tag": 67,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "internal-fe-localself": {
            "class": "SelfIp",
            "address": "10.1.20.4/24",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "internal-fe-tg1-self": {
            "class": "SelfIp",
            "address": "10.1.20.10/24",
            "vlan": "internal",
            "allowService": "none",
            "trafficGroup": "traffic-group-1"
        },
        "external": {
            "class": "VLAN",
            "tag": 9,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "external-localself": {
            "class": "SelfIp",
            "address": "10.1.10.4/24",
            "vlan": "external",
            "allowService": "all",
            "trafficGroup": "traffic-group-local-only"
        },
        "external-tg1-self": {
            "class": "SelfIp",
            "address": "10.1.10.10/24",
            "vlan": "external",
            "allowService": "none",
            "trafficGroup": "traffic-group-1"
        },
        "default": {
            "class": "Route",
            "gw": "10.1.10.1",
            "network": "default",
            "mtu": 1500
        },
        "eRoutingBGP": {
            "class": "RoutingBGP",
            "localAS": 65001,
            "addressFamilies": [
                {
                    "internetProtocol": "ipv4",
                    "redistributionList": [
                        {
                            "routingProtocol": "kernel"
                        }
                    ]
                }
            ],
            "neighbors": [
                {
                    "address": "10.1.10.20",
                    "peerGroup": "Neighbor"
                }
            ],
            "peerGroups": [
                {
                    "name": "Neighbor",
                    "addressFamilies": [
                        {
                            "internetProtocol": "ipv4",
                            "softReconfigurationInboundEnabled": true
                        }
                    ],
                    "remoteAS": 65002
                }
            ],
            "routerId": "10.1.10.4"
        },
        "configsync": {
            "class": "ConfigSync",
            "configsyncIp": "/Common/internal-fe-localself/address"
        },
        "failoverAddress": {
            "class": "FailoverUnicast",
            "address": "/Common/internal-fe-localself/address"
        },
        "failoverGroup": {
            "class": "DeviceGroup",
            "type": "sync-failover",
            "members": [
                "ltm01.f5demo.com",
                "ltm02.f5demo.com",
                "ltm03.f5demo.com"
            ],
            "owner": "/Common/failoverGroup/members/0",
            "autoSync": true,
            "saveOnAutoSync": false,
            "networkFailover": true,
            "fullLoadOnSync": false,
            "asmSync": false
        },
        "trust": {
            "class": "DeviceTrust",
            "localUsername": "admin",
            "localPassword": "<Password>",
            "remoteHost": "10.1.20.5",
            "remoteUsername": "admin",
            "remotePassword": "<Password>"
        }
    }
}