{
    "schemaVersion": "1.23.0",
    "class": "Device",
    "async": true,
    "label": "Basic onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "r01.f5demo.com",
        "myLicense": {
            "class": "License",
            "licenseType": "regKey",
            "regKey": "<License-Key>",
            "overwrite": true
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
              "4.2.2.2",
              "8.8.8.8"
            ],
            "search": [
                "f5demo.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
              "0.pool.ntp.org",
              "1.pool.ntp.org",
              "2.pool.ntp.org"
            ],
            "timezone": "America/Los_Angeles"
        },
        "admin": {
            "class": "User",
            "userType": "regular",
            "shell": "bash"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal"
        },
        "external": {
            "class": "VLAN",
            "tag": 9,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "external-localself": {
            "class": "SelfIp",
            "address": "10.1.10.20/24",
            "vlan": "external",
            "allowService": "all",
            "trafficGroup": "traffic-group-local-only"
        },
        "default": {
            "class": "Route",
            "gw": "10.1.10.1",
            "network": "default",
            "mtu": 1500
        },
        "eRoutingBGP": {
            "class": "RoutingBGP",
            "localAS": 65002,
            "addressFamilies": [
                {
                    "internetProtocol": "ipv4"
                }
            ],
            "neighbors": [
                {
                    "address": "10.1.10.4",
                    "peerGroup": "Neighbor"
                },
                {
                    "address": "10.1.10.5",
                    "peerGroup": "Neighbor"
                },
                {
                    "address": "10.1.10.6",
                    "peerGroup": "Neighbor"
                }
            ],
            "peerGroups": [
                {
                    "name": "Neighbor",
                    "addressFamilies": [
                        {
                            "internetProtocol": "ipv4",
                            "softReconfigurationInboundEnabled": true
                        }
                    ],
                    "remoteAS": 65001
                }
            ],
            "routerId": "10.1.10.20"
        }
    }
}
